{
    "manifest_version": 2,
    "name": "Youtube on speeddial",
    "description": "Shows videos from Youtube subscriptions on speeddial",
    "version": "0.8",
    "developer": {
        "name": "demalexx"
    },
    "background": {
        "page": "index.html"
    },
    "icons": {
        "128": "icons/extension-icon.png"
    },
    "permissions": [
        "speeddial",
        "storage",
        "*://demalexx.github.io/youtube-opera-speeddial/selectors.json",
        "*://www.youtube.com/feed/subscriptions",
        "*://*.ytimg.com/*",
        "*://*.ggpht.com/*"
    ],
    "speeddial": {
        "url": "https://www.youtube.com/feed/subscriptions"
    }
}
